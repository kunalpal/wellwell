# === wellwell:begin ===
{{#if paths}}
export PATH="{{paths}}:$PATH"
{{else}}
export PATH="$HOME/bin:$PATH"
{{/if}}

{{#each aliases}}
alias {{name}}="{{value}}"
{{/each}}

{{#each envVars}}
export {{name}}="{{value}}"
{{/each}}

{{#each shellInit}}
{{{initCode}}}
{{/each}}

{{#if isMacos}}
export BROWSER="open"
{{/if}}

# Source local machine-specific overrides if present
[ -f "$HOME/.ww-overrides.zsh" ] && source "$HOME/.ww-overrides.zsh"
# === wellwell:end ===
