    "wellwell": "bin/wellwell"
  },
  "dependencies": {
    "ink": "^6.1.0",
    "meow": "^13.2.0",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "tsup": "^8.5.0",
